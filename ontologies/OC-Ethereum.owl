    <!-- http://www.dmi.unict.it/oasis.owl#isDescribedBy -->

    <owl:ObjectProperty rdf:about="http://www.dmi.unict.it/oasis.owl#isDescribedBy"/>
    


    <!-- http://www.dmi.unict.it/oasis.owl#isRelatedWith -->

