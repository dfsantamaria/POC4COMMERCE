    <!-- http://www.ngi.ontochain/ontologies/oc-ethereum.owl#hasEthereumTokenPerdurantFeature -->

    <owl:ObjectProperty rdf:about="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#hasEthereumTokenPerdurantFeature">
        <rdfs:subPropertyOf rdf:resource="http://www.dmi.unict.it/oasis.owl#isRelatedWith"/>
        <rdfs:domain rdf:resource="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#EthereumToken"/>
        <rdfs:range rdf:resource="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#EthereumTokenPerdurantFeature"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ngi.ontochain/ontologies/oc-ethereum.owl#hasEthereumTokenFeatureModificationResult -->

    <owl:ObjectProperty rdf:about="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#hasEthereumTokenFeatureModificationResult">
        <rdfs:subPropertyOf rdf:resource="http://www.dmi.unict.it/oasis.owl#involves"/>
        <rdfs:domain rdf:resource="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#EthereumTokenFeatureModificationActivity"/>
        <rdfs:range rdf:resource="http://www.dmi.unict.it/oasis.owl#PerdurantFeature"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ngi.ontochain/ontologies/oc-ethereum.owl#hasEthereumTokenFeatureModificationSource -->

    <owl:ObjectProperty rdf:about="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#hasEthereumTokenFeatureModificationSource">
        <rdfs:subPropertyOf rdf:resource="http://www.dmi.unict.it/oasis.owl#involves"/>
        <rdfs:domain rdf:resource="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#EthereumTokenFeatureModificationActivity"/>
        <rdfs:range rdf:resource="http://www.dmi.unict.it/oasis.owl#PerdurantFeature"/>
        <rdfs:range rdf:resource="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#DeprecatedEthereumTokenPerdurantFeature"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ngi.ontochain/ontologies/oc-ethereum.owl#introducesEthereumSmartContractAgent -->

    <owl:ObjectProperty rdf:about="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#introducesEthereumSmartContractAgent">
        <rdfs:subPropertyOf rdf:resource="http://www.dmi.unict.it/oasis.owl#describes"/>
        <rdfs:domain rdf:resource="http://www.semanticblockchain.com/Blondie.owl#ContractCreationEthereumTransaction"/>
        <rdfs:range rdf:resource="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#EthereumSmartContractAgent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ngi.ontochain/ontologies/oc-ethereum.owl#introducesEthereumSmartContractExecution -->

    <owl:ObjectProperty rdf:about="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#introducesEthereumSmartContractExecution">
        <rdfs:subPropertyOf rdf:resource="http://www.dmi.unict.it/oasis.owl#describes"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticblockchain.com/Blondie.owl#MessageCallEthereumTransaction"/>
                    <rdf:Description rdf:about="http://www.semanticblockchain.com/Blondie.owl#NormalEthereumTransaction"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#EthereumSmartContractExecution"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ngi.ontochain/ontologies/oc-ethereum.owl#isDescribedByEthereumToken -->

    <owl:ObjectProperty rdf:about="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#isDescribedByEthereumToken">
        <rdfs:subPropertyOf rdf:resource="http://www.dmi.unict.it/oasis.owl#isDescribedBy"/>
        <rdfs:domain rdf:resource="http://www.ngi.ontochain/ontologies/oc-commerce.owl#CommercialAsset"/>
        <rdfs:range rdf:resource="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#EthereumToken"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ngi.ontochain/ontologies/oc-ethereum.owl#isEthereumTokenFeatureModifiedIn -->

    <owl:ObjectProperty rdf:about="http://www.ngi.ontochain/ontologies/oc-ethereum.owl#isEthereumTokenFeatureModifiedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.dmi.unict.it/oasis.owl#involves"/>
        <rdfs:domain rdf:resource="http://www.dmi.unict.it/oasis.owl#PerdurantFeature"/>
        <rdfs:range rdf:resource="http://www.dmi.unict.it/oasis.owl#PerdurantFeature"/>
    </owl:ObjectProperty>
    


